{
  "include": [
    "src"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ],
  "compilerOptions": {
    "noImplicitAny": true,
    "outDir": "dist",
    "module": "commonjs",
    "strict": true,
    "strictNullChecks": true,
    "lib": ["es2015"],
    "target": "es2015",
    "declaration": true
  },
  "compileOnSave": false
}
